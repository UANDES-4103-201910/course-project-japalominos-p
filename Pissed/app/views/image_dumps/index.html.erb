<p id="notice"><%= notice %></p>

<h1>Image Dumps</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_dumps.each do |image_dump| %>
      <tr>
        <td><%= image_dump.url %></td>
        <td><%= link_to 'Show', image_dump %></td>
        <td><%= link_to 'Edit', edit_image_dump_path(image_dump) %></td>
        <td><%= link_to 'Destroy', image_dump, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image Dump', new_image_dump_path %>
